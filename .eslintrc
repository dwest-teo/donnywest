{
  "globals": {
    "document": true,
    "window": true
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "jsx": true
  },
  "plugins": [
    "react",
    "import",
    "jsx-a11y"
  ],
  "extends": "airbnb",
  "rules": {
    "strict": 0,
    "react/jsx-filename-extension": 0,
    "max-len": 1
  }
}
